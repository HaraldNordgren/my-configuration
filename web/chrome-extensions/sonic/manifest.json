{
  "name": "Sonic",
  "version": "0.1",
  "manifest_version": 2,
  "description": "My first Chrome extension.",
  "permissions": [ "tabs" ],

  "browser_action": {
    "default_icon": "Sonic_Sprite.png",
    "default_popup": "helloworld.html"
  },
  
  "content_scripts": [
    {
      "matches": ["http://stackoverflow.com/questions/*/*"],
      "js": ["content.js"]
    }
  ]
}
